@{
    ViewData["Title"] = "CMS Page";
    Layout = "~/Views/backend/layout.cshtml";

}
@model IEnumerable<WebApplication2.Models.RoleClaimtable>

<h1>Permissions</h1>
<br />
<div class="card">

 

    <div id="viewAll" class="card-body table-responsive">
 
            <table class="table table-striped" id="permissionTable">
                <thead>
                    <tr>
                        <th>
                            Type
                        </th>
                        <th>
                            Permission
                        </th>
                   
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Type)

                            </td>
                           
                            <td>
                                @Html.DisplayFor(modelItem => item.Value)

                            </td>
                        </tr>
                    }

                </tbody>
            </table>
